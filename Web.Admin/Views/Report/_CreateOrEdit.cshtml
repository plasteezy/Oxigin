@model Web.Data.Model.SavedQuery

@* This partial view defines form fields that will appear when creating and editing entities *@
<div class="form-group">
	<label for="Subject">Description:</label>
		@Html.EditorFor(model => model.Description, new { htmlAttributes = new {@class= "form-control"}})
		@Html.ValidationMessageFor(model => model.Description)
</div>
<div class="form-group">
    <label for="QueryBody">Query Body:</label>
        @Html.EditorFor(model => model.Definition, new { htmlAttributes = new { @class = "form-control" }})
        @Html.ValidationMessageFor(model => model.Definition)
</div>
<div class="form-group">
    <label for="QueryBody">Configuration:</label>
    @Html.EditorFor(model => model.Config, new {htmlAttributes = new {@class = "form-control"}})
    @Html.ValidationMessageFor(model => model.Config)
</div>


